msgid "Download"
msgstr "Scarica"

#: trunk/zp-core/admin-options.php:1441
msgid "No access"
msgstr "Nessun accesso"

#: trunk/zp-core/admin-options.php:1447
msgid "Disable hotlinking"
msgstr "Disabilita hotlink"

#: trunk/zp-core/admin-options.php:1452
